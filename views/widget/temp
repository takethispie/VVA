    </div>
    <div class="col-sm-2 text-center">
        <p class="user"><%= temp.USER%></p>
    </div>
    <div class="col-sm-1">
        <p class="nometatresa"><%= temp.NOMETATRESA%></p>
    </div>
    <div class="col-sm-1">
        <p class="dateresa"><%= temp.DATERESA%></p>
    </div>
    <div class="col-sm-1">
        <p class="dateaccuserecept"><%= temp.DATEACCUSERECEPT%></p>
    </div>
    <div class="col-sm-1">
        <p class="datearrhes"><%= temp.DATEARRHES%></p>
    </div>
